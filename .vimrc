
colorscheme lucius
filetype indent on
set autoindent
set background=dark
set backspace=2
set cursorline
set list
set listchars=eol:¬,tab:>·,trail:·,extends:>,precedes:<
set nobackup
set nowrap
set number
set paste
set tabstop=8 softtabstop=0 expandtab shiftwidth=4 smarttab
syntax on

execute pathogen#infect()

